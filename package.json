{
  "name": "codieumextension",
  "displayName": "codeium",
  "description": "comment coder",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codieumextension.helloWorld",
    "onCommand:codieumextension.showStatus",
    "onCommand:codieumextension.showCodeCommentsPanel"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/NamanBiyani06/codieumextension.git"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codieumextension.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "codieumextension.showStatus",
        "title": "Show Extension Status"
      },
      {
        "command": "codieumextension.showCodeCommentsPanel",
        "title": "Show Code & Comments Side by Side"
      },
      {
        "command": "codieumextension.showUnifiedCommentPanel",
        "title": "Show Unified Comment Panel"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.104.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2",
    "vsce": "^1.103.1"
  }
}
